--TEST--
Bug 480280 - No error for non-abstract method with no body
--FILE--
<?php 
trait Trait1 {
    /* no error check is done here */
    function foo1();

    /* no error check is done here */
    abstract function foo1();

    abstract function foo1() {
    	$a = 0;
    }
}

class MyClass1 {
    use Trait1;

    function foo1();

    abstract function foo1();

    abstract function foo1() {
    	$a = 0;
    }
}

abstract class MyClass2 {
    use Trait1;

    function foo1();

    abstract function foo1() {
    	$a = 0;
    }
}
?>
