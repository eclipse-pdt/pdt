--TEST--
Test Constant Scalar Expressions
--FILE--
<?php
const BAR = 0x10;
 
function foo() {
    	static $a = 1 **      1;
    static $b = [1 	<< 2];
    	static $c = 0x01 | 	BAR;
}
?>
--EXPECT--
<?php
const BAR = 0x10;
function foo() {
	static $a = 1 ** 1;
	static $b = [ 
			1 << 2
	];
	static $c = 0x01 | BAR;
}
?>