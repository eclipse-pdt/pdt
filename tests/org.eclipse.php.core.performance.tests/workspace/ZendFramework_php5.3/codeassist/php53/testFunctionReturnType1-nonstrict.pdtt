--TEST--
Should complete namespace methods from function return type
--PREFERENCES--
org.eclipse.php.core/contentAssistShowStrictOptions=false
--FILE--
<?
namespace NS;
class A { function foo() {} }

namespace OtherNS;
class B { function bar() {} }

namespace ThirdNS;
use OtherNS as Z;
class C { function boo() {} }

function bar() {
	if (m()) {
		return new C();
	} else if (t()) {
		return new \NS\A();
	} else {
		return new Z\B();
	}
}

namespace FourthNS;
\ThirdNS\bar()->|

?>
--EXPECT--
method(foo)
method(bar)
method(boo)
