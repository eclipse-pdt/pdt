{
	"comments": {
		"blockComment": [ "{#", "#}" ]
	},
	"brackets": [
		["{{", "}}"],
		["{%", "%}"],
		["{", "}"],
		["[", "]"],
		["(", ")"]
	],
	"surroundingPairs": [
		{ "open": "{{", "close": "}}"},
		{ "open": "{%", "close": "%}"},
		{ "open": "'", "close": "'", "notIn" : ["text.html.twig"] },
		{ "open": "\"", "close": "\"", "notIn" : ["text.html.twig"] },
		{ "open": "{", "close": "}", "notIn" : ["text.html.twig"]},
		{ "open": "[", "close": "]", "notIn" : ["text.html.twig"]},
		{ "open": "(", "close": ")", "notIn" : ["text.html.twig"] },
		{ "open": "<", "close": ">", "notIn" : ["text.html.twig"] }
	]
}