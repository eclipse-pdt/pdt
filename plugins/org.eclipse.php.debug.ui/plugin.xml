            <view id="org.eclipse.debug.ui.PHPDebugOutput" 
                relative="org.eclipse.ui.console.ConsoleView" 
                relationship="stack" 
            />
            <showInPart id="org.eclipse.ui.views.ResourceNavigator"/> 
