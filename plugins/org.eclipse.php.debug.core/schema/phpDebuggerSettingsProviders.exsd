      <complexType>
         <sequence minOccurs="1" maxOccurs="unbounded">
            <element ref="phpDebuggerSettingsProvider" minOccurs="1" maxOccurs="unbounded"/>
         </sequence>
         <attribute name="point" type="string" use="required">
            <annotation>
               <documentation>
                  
               </documentation>
            </annotation>
         </attribute>
         <attribute name="id" type="string">
            <annotation>
               <documentation>
                  
               </documentation>
            </annotation>
         </attribute>
         <attribute name="name" type="string">
            <annotation>
               <documentation>
                  
               </documentation>
               <appInfo>
                  <meta.attribute translatable="true"/>
               </appInfo>
            </annotation>
         </attribute>
      </complexType>
   </element>

   <element name="phpDebuggerSettingsProvider">
      <annotation>
         <documentation>
            PHP debugger settings provider definition.
         </documentation>
      </annotation>
      <complexType>
         <attribute name="id" type="string" use="required">
            <annotation>
               <documentation>
                  Unique ID of debugger settings provider.
               </documentation>
            </annotation>
         </attribute>
         <attribute name="debuggerId" type="string" use="required">
