	<target name="jdtcorecompiler">
		<condition property="jvm" value="${J2SE-5.0}">
			<isset property="J2SE-5.0"/>
		</condition>
		<antcall target="runtests" >
			<param name="testPlugin" value="${org.eclipse.jdt.core.tests.compiler}" />
